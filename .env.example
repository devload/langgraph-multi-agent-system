# Multi-Agent Mission System Environment Variables

# Hub Configuration
HUB_HOST=0.0.0.0
HUB_PORT=8000
HUB_URL=http://localhost:8000

# Agent Configuration
CLAUDE_AGENT_PORT=8001
GEMINI_AGENT_PORT=8002

# Security
API_KEY=your-secret-api-key-here
ENABLE_AUTH=false

# Database
DATABASE_PATH=hub/missions.db

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/system.log

# CLI Configuration
CLAUDE_CLI_PATH=claude
GEMINI_CLI_PATH=gemini

# Workspace
AGENT_WORKSPACE_ROOT=/tmp/agent-workspaces

# Performance
MAX_CONCURRENT_MISSIONS=10
MISSION_TIMEOUT=600
AGENT_TIMEOUT=300